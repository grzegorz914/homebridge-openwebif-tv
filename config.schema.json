{
  "pluginAlias": "OpenWebIfTv",
  "pluginType": "platform",
  "singular": true,
  "fixArrays": true,
  "strictValidation": true,
  "headerDisplay": "This plugin works with Sat Receivers basis on the OpenWebIf interface. Devices are exposed to HomeKit as separate accessories and each needs to be manually paired.",
  "footerDisplay": "For documentation please see [GitHub repository](https://github.com/grzegorz914/homebridge-openwebif-tv).",
  "schema": {
    "type": "object",
    "properties": {
      "devices": {
        "type": "array",
        "items": {
          "title": "Device",
          "type": "object",
          "properties": {
            "name": {
              "title": "Name",
              "type": "string",
              "placeholder": "Sat Receiver"
            },
            "host": {
              "title": "IP/Hostname",
              "type": "string",
              "placeholder": "192.160.1.10",
              "format": "hostname"
            },
            "port": {
              "title": "Port",
              "type": "integer",
              "placeholder": 80
            },
            "auth": {
              "title": "Authorization",
              "type": "object",
              "properties": {
                "enable": {
                  "title": "Enable",
                  "type": "boolean",
                  "default": false
                },
                "user": {
                  "title": "User",
                  "type": "string",
                  "placeholder": "User",
                  "condition": {
                    "functionBody": "return model.devices[arrayIndices].auth.enable === true;"
                  }
                },
                "passwd": {
                  "title": "Password",
                  "type": "string",
                  "placeholder": "Password",
                  "format": "password",
                  "condition": {
                    "functionBody": "return model.devices[arrayIndices].auth.enable === true;"
                  }
                }
              }
            },
            "displayType": {
              "title": "Accessory type",
              "type": "integer",
              "default": 2,
              "description": "Accessory type for Home app",
              "oneOf": [
                {
                  "title": "None/Disabled",
                  "enum": [
                    0
                  ]
                },
                {
                  "title": "Television",
                  "enum": [
                    1
                  ]
                },
                {
                  "title": "TV Set Top Box",
                  "enum": [
                    2
                  ]
                },
                {
                  "title": "TV Streaming Stick",
                  "enum": [
                    3
                  ]
                },
                {
                  "title": "Audio Receiver",
                  "enum": [
                    4
                  ]
                }
              ]
            },
            "inputs": {
              "title": "Inputs",
              "type": "object",
              "properties": {
                "getFromDevice": {
                  "title": "Load From Device",
                  "type": "boolean",
                  "default": false,
                  "description": "This function get all available inputs direct from device, manually configured inputs will be skipped."
                },
                "displayOrder": {
                  "title": "Display Order",
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 4,
                  "default": 0,
                  "description": "Here select display order of the inputs list.",
                  "oneOf": [
                    {
                      "title": "None",
                      "enum": [
                        0
                      ]
                    },
                    {
                      "title": "Ascending by Name",
                      "enum": [
                        1
                      ]
                    },
                    {
                      "title": "Descending by Name",
                      "enum": [
                        2
                      ]
                    },
                    {
                      "title": "Ascending by Reference",
                      "enum": [
                        3
                      ]
                    },
                    {
                      "title": "Descending by Reference",
                      "enum": [
                        4
                      ]
                    }
                  ]
                },
                "bouquets": {
                  "title": "Bouquets",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "title": "Name",
                        "type": "string",
                        "placeholder": "Bouquet name",
                        "description": "Here set bouquet name which should be loaded from device, only first 85 channels will be used."
                      },
                      "displayType": {
                        "title": "Display Type",
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 2,
                        "default": 0,
                        "description": "Here select extra tile for all channels of this bouquet to be exposed in HomeKit app.",
                        "oneOf": [
                          {
                            "title": "None/Disabled",
                            "enum": [
                              0
                            ]
                          },
                          {
                            "title": "Outlet",
                            "enum": [
                              1
                            ]
                          },
                          {
                            "title": "Switch",
                            "enum": [
                              2
                            ]
                          }
                        ]
                      },
                      "namePrefix": {
                        "title": "Prefix",
                        "type": "boolean",
                        "description": "Here enable/disable the accessory name as a prefix for extra tile name."
                      }
                    },
                    "required": [
                      "displayType"
                    ]
                  },
                  "condition": {
                    "functionBody": "return model.devices[arrayIndices].inputs.getFromDevice === true;"
                  }
                },
                "channels": {
                  "title": "Channels",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "title": "Name",
                        "type": "string",
                        "placeholder": "Channel name",
                        "description": "Here set Your own name."
                      },
                      "reference": {
                        "title": "Reference",
                        "type": "string",
                        "placeholder": "Channel reference",
                        "description": "Here set the reference."
                      },
                      "displayType": {
                        "title": "Extra Tile",
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 2,
                        "default": 0,
                        "description": "Here select extra tile to be exposed in HomeKit app.",
                        "oneOf": [
                          {
                            "title": "None/Disabled",
                            "enum": [
                              0
                            ]
                          },
                          {
                            "title": "Outlet",
                            "enum": [
                              1
                            ]
                          },
                          {
                            "title": "Switch",
                            "enum": [
                              2
                            ]
                          }
                        ]
                      },
                      "namePrefix": {
                        "title": "Prefix",
                        "type": "boolean",
                        "description": "Here enable/disable the accessory name as a prefix for extra tile name."
                      }
                    },
                    "required": [
                      "displayType"
                    ]
                  },
                  "condition": {
                    "functionBody": "return model.devices[arrayIndices].inputs.getFromDevice === false;"
                  }
                }
              },
              "required": [
                "displayOrder"
              ]
            },
            "buttons": {
              "title": "Button",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "displayType": {
                    "title": "Display Type",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 2,
                    "default": 0,
                    "oneOf": [
                      {
                        "title": "None/Disabled",
                        "enum": [
                          0
                        ]
                      },
                      {
                        "title": "Outlet",
                        "enum": [
                          1
                        ]
                      },
                      {
                        "title": "Switch",
                        "enum": [
                          2
                        ]
                      }
                    ],
                    "description": "Here select display type in HomeKit app"
                  },
                  "name": {
                    "title": "Name",
                    "type": "string",
                    "placeholder": "Button name",
                    "description": "Here set Your own name.",
                    "condition": {
                      "functionBody": "return model.devices[arrayIndices[0]].buttons[arrayIndices[1]].displayType > 0;"
                    }
                  },
                  "mode": {
                    "title": "Mode",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 2,
                    "description": "Here select the function mode.",
                    "oneOf": [
                      {
                        "title": "Live TV Channel",
                        "enum": [
                          0
                        ]
                      },
                      {
                        "title": "Remote Control",
                        "enum": [
                          1
                        ]
                      },
                      {
                        "title": "Power Control",
                        "enum": [
                          2
                        ]
                      }
                    ],
                    "condition": {
                      "functionBody": "return model.devices[arrayIndices[0]].buttons[arrayIndices[1]].displayType > 0;"
                    }
                  },
                  "reference": {
                    "title": "Reference",
                    "type": "string",
                    "placeholder": "Button reference",
                    "description": "Here set the reference.",
                    "condition": {
                      "functionBody": "return model.devices[arrayIndices[0]].buttons[arrayIndices[1]].displayType > 0 && model.devices[arrayIndices[0]].buttons[arrayIndices[1]].mode === 0;"
                    }
                  },
