                    "condition": {
                      "functionBody": "return model.devices[arrayIndices[0]].buttons[arrayIndices[1]].displayType > 0 && model.devices[arrayIndices[0]].buttons[arrayIndices[1]].mode === 1;"
                    }
                  },
                  "powerCommand": {
                    "title": "Power Command",
                    "type": "string",
                    "description": "Here select the power control command.",
