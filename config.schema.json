                "channel": {
                  "title": "Channel",
                  "type": "boolean",
                  "default": false,
                  "description": "This expose Channel as a Motion Sensor to use with automations."
                },
                "channels": {
                  "type": "array",
                  "items": {
                    "title": "Channels",
                    "type": "object",
                    "description": "Here create Channels Sensors based on reference, sensor will active if switch to it.",
